javascript:(function(){(function($){$.fn.showIids=function(){var%20pageType=digitalData.page.category.pageType;var%20descriptionCode=digitalData.page.attributes.descriptonCode;return%20this.each(function(i,el){var%20link=el.href;var%20orgText=el.innerHTML;var%20offerType=(el.attributes['data-offertype'].value);var%20componet=(el.attributes['data-component'].value);if(link.indexOf("/videos/")!=-1){offerType="vid";}else%20if(link.indexOf("/company/events/conferences")!=-1){offerType="conf";}else%20if(link.indexOf("/academia/courseware/")!=-1){offerType="cw";}else%20if(link.indexOf("-sem-")!=-1){offerType="sem";}else%20if(link.indexOf("-lwb-")!=-1){offerType="lwb";}else%20if(link.indexOf("/campaigns/")!=-1&&link.indexOf("/offer/")!=-1){offerType="cmpofr";}else%20if(link.indexOf("/campaigns/")!=-1&&link.indexOf("/display/")!=-1){offerType="cmpdsp";}else%20if(link.indexOf("/campaigns/")!=-1&&link.indexOf("/ppc/")!=-1){offerType="cmpppc";}else{offerType.replace(/\s/g,"");}el.onmouseover=function(){el.innerHTML=pageType+"_"+offerType+"_"+descriptionCode+"_"+componet;};el.onmouseout=function(){el.innerHTML=orgText;};});return%20this;};})(jQuery);jQuery('a[data-link="lead"]').showIids().css("color","red");})();
